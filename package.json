{
  "name": "frankowebapp",
  "version": "1.0.0",
  "description": "Franko Admin Web App",
  "main": "main.js",
  "author": "Franko Trading",
  "license": "ISC",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "electron-builder"
  },
  "devDependencies": {
    "electron": "^36.5.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.frankotrading.admin",
    "productName": "Franko Admin",
    "files": [
      "**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icon.png"
    },
    "rcedit": null,
    "publish": {
      "provider": "github",
      "owner": "Owusua22",
      "repo": "webApp"
    }
  }
}
